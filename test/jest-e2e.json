{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "preset": "ts-jest",
  "globalSetup": "<rootDir>/globalSetup.ts",
  "globalTeardown": "<rootDir>/globalTeardown.ts",
  "setupFilesAfterEnv": [
    "<rootDir>/setupFile.ts"
  ]
}
